    "import numpy as np\n",
    "import sys\n",
    "\n",
    "path = \"output.tmp\"\n",
    "with open(path, 'r') as f:\n",
    "    text_scores = f.readlines()\n",
    "    num_scores = [float(s) for s in text_scores]\n",
    "\n",
    "    print \"Min score is \", min(num_scores)\n",
    "    print \"Index of it was\", np.argmin(num_scores)"
